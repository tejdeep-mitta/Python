pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        	
         stage(copy){
            steps {
            echo 'hi tejdeep hi'
            mkdir new
           
            
            }
        }
		
		stage(prebuild)
        {
        steps{
        dir('sample files') { 
		git branch: 'main',  url: 'https://github.com/tejdeep-mitta/Python'
		}
		}
		}
		
    }
}
